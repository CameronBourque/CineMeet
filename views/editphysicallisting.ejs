<h1 class="mt-4" style="font-family:'Calibri'">CineMeet</h1>
<p class="lead mb-3" style="font-family:'Calibri'">Edit physical movie meetup listing.</p>
<div class="row mt-5" style="font-family:'Calibri'">
    <div class="col-md-10 m-auto">
        <div class="card card-body">
            <% include ./partials/messages %>
            <form action="/listings/updatephysical" method="POST">
                <div class="form-group col-md-6" type="hidden">
                    <input
                            type="hidden"
                            id="id"
                            name="id"
                            class="form-control"
                            value="<%= listing.id %>"
                    />
                    <input
                            type="hidden"
                            id="src"
                            name="src"
                            value="<%= src %>"
                    />
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6" >
                        <label for="listingname">Listing Name</label>
                        <input
                                type="text"
                                id="listingname"
                                name="listingname"
                                class="form-control"
                                value="<%=listing.listingName%>"
                                placeholder="Enter listing name"
                        />
                    </div>
                    <div class="form-group col-md-6">
                        <label for="moviename">Movie Name</label>
                        <input
                                type="text"
                                id="moviename"
                                name="moviename"
                                class="form-control"
                                value="<%= listing.movieName %>"
                                placeholder="Enter movie name"
                        />
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6" >
                        <label for="date">Date</label>
                        <input
                                type="date"
                                id="date"
                                name="date"
                                class="form-control"
                                value="<%= listing.date %>"
                                placeholder="Enter date"
                        />
                    </div>
                    <div class="form-group col-md-6">
                        <label for="time">Time</label>
                        <input
                                type="time"
                                id="time"
                                name="time"
                                class="form-control"
                                value="<%= listing.time %>"
                                placeholder="Enter time"
                        />
                    </div>
                </div>
                <div class="form-group" >
                    <label for="venue">Venue</label>
                    <input
                            type="text"
                            id="venue"
                            name="venue"
                            class="form-control"
                            value="<%= listing.venueName %>"
                            placeholder="Enter venue name"
                    />
                </div>
                <div class="form-group">
                    <label for="address">Address</label>
                    <input
                            type="text"
                            id="address"
                            name="address"
                            class="form-control"
                            value="<%= listing.address %>"
                            placeholder="Enter address"
                    />
                </div>
                <div class="form-group">
                    <label for="address2">Address 2</label>
                    <input
                            type="text"
                            id="address2"
                            name="address2"
                            class="form-control"
                            value="<%= listing.address2 %>"
                            placeholder="Apartment, studio, or floor"
                    />
                </div>
                <div class="form-row">
                    <div class="form-group col-md-4" >
                        <label for="city">City</label>
                        <input
                                type="text"
                                id="city"
                                name="city"
                                class="form-control"
                                value="<%= listing.city %>"
                                placeholder="Enter city"
                        />
                    </div>
                    <div class="form-group col-md-4">
                        <label for="state">State</label>
                        <select id="state" name="state" class="form-control">
                            <option value="AK" <%if(listing.state === "AK") {%>selected="true"<%}%>>Alaska</option>
                            <option value="AL" <%if(listing.state === "AL") {%>selected="true"<%}%>>Alabama</option>
                            <option value="AR" <%if(listing.state === "AR") {%>selected="true"<%}%>>Arkansas</option>
                            <option value="AZ" <%if(listing.state === "AZ") {%>selected="true"<%}%>>Arizona</option>
                            <option value="CA" <%if(listing.state === "CA") {%>selected="true"<%}%>>California</option>
                            <option value="CO" <%if(listing.state === "CO") {%>selected="true"<%}%>>Colorado</option>
                            <option value="CT" <%if(listing.state === "CT") {%>selected="true"<%}%>>Connecticut</option>
                            <option value="DC" <%if(listing.state === "DC") {%>selected="true"<%}%>>District of Columbia</option>
                            <option value="DE" <%if(listing.state === "DE") {%>selected="true"<%}%>>Delaware</option>
                            <option value="FL" <%if(listing.state === "FL") {%>selected="true"<%}%>>Florida</option>
                            <option value="GA" <%if(listing.state === "GA") {%>selected="true"<%}%>>Georgia</option>
                            <option value="HI" <%if(listing.state === "HI") {%>selected="true"<%}%>>Hawaii</option>
                            <option value="IA" <%if(listing.state === "IA") {%>selected="true"<%}%>>Iowa</option>
                            <option value="ID" <%if(listing.state === "ID") {%>selected="true"<%}%>>Idaho</option>
                            <option value="IL" <%if(listing.state === "IL") {%>selected="true"<%}%>>Illinois</option>
                            <option value="IN" <%if(listing.state === "IN") {%>selected="true"<%}%>>Indiana</option>
                            <option value="KS" <%if(listing.state === "KS") {%>selected="true"<%}%>>Kansas</option>
                            <option value="KY" <%if(listing.state === "KY") {%>selected="true"<%}%>>Kentucky</option>
                            <option value="LA" <%if(listing.state === "LA") {%>selected="true"<%}%>>Louisiana</option>
                            <option value="MA" <%if(listing.state === "MA") {%>selected="true"<%}%>>Massachusetts</option>
                            <option value="MD" <%if(listing.state === "MD") {%>selected="true"<%}%>>Maryland</option>
                            <option value="ME" <%if(listing.state === "ME") {%>selected="true"<%}%>>Maine</option>
                            <option value="MI" <%if(listing.state === "MI") {%>selected="true"<%}%>>Michigan</option>
                            <option value="MN" <%if(listing.state === "MN") {%>selected="true"<%}%>>Minnesota</option>
                            <option value="MO" <%if(listing.state === "MO") {%>selected="true"<%}%>>Missouri</option>
                            <option value="MS" <%if(listing.state === "MS") {%>selected="true"<%}%>>Mississippi</option>
                            <option value="MT" <%if(listing.state === "MT") {%>selected="true"<%}%>>Montana</option>
                            <option value="NC" <%if(listing.state === "NC") {%>selected="true"<%}%>>North Carolina</option>
                            <option value="ND" <%if(listing.state === "ND") {%>selected="true"<%}%>>North Dakota</option>
                            <option value="NE" <%if(listing.state === "NE") {%>selected="true"<%}%>>Nebraska</option>
                            <option value="NH" <%if(listing.state === "NH") {%>selected="true"<%}%>>New Hampshire</option>
                            <option value="NJ" <%if(listing.state === "NJ") {%>selected="true"<%}%>>New Jersey</option>
                            <option value="NM" <%if(listing.state === "NM") {%>selected="true"<%}%>>New Mexico</option>
                            <option value="NV" <%if(listing.state === "NV") {%>selected="true"<%}%>>Nevada</option>
                            <option value="NY" <%if(listing.state === "NY") {%>selected="true"<%}%>>New York</option>
                            <option value="OH" <%if(listing.state === "OH") {%>selected="true"<%}%>>Ohio</option>
                            <option value="OK" <%if(listing.state === "OK") {%>selected="true"<%}%>>Oklahoma</option>
                            <option value="OR" <%if(listing.state === "OR") {%>selected="true"<%}%>>Oregon</option>
                            <option value="PA" <%if(listing.state === "PA") {%>selected="true"<%}%>>Pennsylvania</option>
                            <option value="PR" <%if(listing.state === "PR") {%>selected="true"<%}%>>Puerto Rico</option>
                            <option value="RI" <%if(listing.state === "RI") {%>selected="true"<%}%>>Rhode Island</option>
                            <option value="SC" <%if(listing.state === "SC") {%>selected="true"<%}%>>South Carolina</option>
                            <option value="SD" <%if(listing.state === "SD") {%>selected="true"<%}%>>South Dakota</option>
                            <option value="TN" <%if(listing.state === "TN") {%>selected="true"<%}%>>Tennessee</option>
                            <option value="TX" <%if(listing.state === "TX") {%>selected="true"<%}%>>Texas</option>
                            <option value="UT" <%if(listing.state === "UT") {%>selected="true"<%}%>>Utah</option>
                            <option value="VA" <%if(listing.state === "VA") {%>selected="true"<%}%>>Virginia</option>
                            <option value="VT" <%if(listing.state === "VT") {%>selected="true"<%}%>>Vermont</option>
                            <option value="WA" <%if(listing.state === "WA") {%>selected="true"<%}%>>Washington</option>
                            <option value="WI" <%if(listing.state === "WI") {%>selected="true"<%}%>>Wisconsin</option>
                            <option value="WV" <%if(listing.state === "WV") {%>selected="true"<%}%>>West Virginia</option>
                            <option value="WY" <%if(listing.state === "WY") {%>selected="true"<%}%>>Wyoming</option>
                        </select>
                    </div>
                    <div class="form-group col-md-4" >
                        <label for="zipcode">Zipcode</label>
                        <input
                                type="text"
                                id="zipcode"
                                name="zipcode"
                                class="form-control"
                                value="<%= listing.zipcode %>"
                                placeholder="Enter zipcode"
                        />
                    </div>
                </div>
                <div class="form-group">
                    <label for="eventtype">Event Type</label>
                    <select id="eventtype" name="eventtype" class="form-control">
                        <option value="private" <%if(listing.status === "private") {%>selected="true"<%}%>>Private</option>
                        <option value="public" <%if(listing.status === "public") {%>selected="true"<%}%>>Public</option>
                    </select>
                </div>
                <button type="submit" name="btype" value="save" class="btn btn-primary btn-block">Save Listing</button>
                <button type="submit" name="btype" value="delete" class="btn btn-primary btn-block">Delete Listing</button>
            </form>
        </div>
    </div>
</div>
